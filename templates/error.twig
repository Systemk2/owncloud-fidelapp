<div id="fidelapp_errors">
	{% if(errors) %}
	<div class="fidelapp_error_messages">
		<ul>
			{% for error in errors %}
			<li>{{ error }}</li> {% endfor %}
		</ul>
	</div>
	{% endif %} {% if(warnings) %}
	<div class="fidelapp_warning_messages">
		<ul>
			{% for warning in warnings %}
			<li>{{ warning }}</li> {% endfor %}
		</ul>
	</div>
	{% endif %}
</div>

